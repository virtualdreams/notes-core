{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build (Debug)",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/Notes.csproj"
			],
			"problemMatcher": "$msCompile"
		},
		{
			"label": "build (Release)",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/Notes.csproj",
				"-c",
				"Release"
			],
			"problemMatcher": "$msCompile"
		}
	]
}